    <script>
      
      function changeQty(userId,productId,qty){
        // console.log("button cliked...");
      let totalDisplay = document.getElementById('totalDisplay')
      let subTotalDisplay = document.getElementById('subTotalDisplay');
      let singleProductTotal = document.getElementById(`singleProductTotal${productId}`)
      let qtyInput = document.getElementById(`qty${productId}`);
      let singleProductPrice = document.getElementById(`singleProductPrice${productId}`).innerHTML;

      if(userId){
        $.ajax({
          url:'/changeqty',
          method:'post',
          data:{userId,productId,qty},
          success:(response)=>{
            // console.log(response.cartItems.products[0].product);
            // console.log(productId);
            response.total==undefined?window.Location.href='/login':
            totalDisplay.innerHTML="₹ "+response.total
            subTotalDisplay.innerHTML="₹ "+response.total

            let newQuantity = response.cartItems.products.find(val=>val.product==productId);
            
            qtyInput.value=newQuantity.quantity
            console.log(singleProductPrice.replace("₹", ""), newQuantity.quantity);
            
            // singleProductTotal.innerHTML = `${Number(singleProductPrice.innerHTML) * Number(newQuantity.quantity)}`;
            singleProductTotal.innerHTML = "₹ "+(Number(singleProductPrice.replace("₹", "")) * newQuantity.quantity);

            
            // console.log("hello:"+singleProductTotal);
          }
        })
      }
      }
    </script>
       

     <script>
    
        
      function removeCartItem(userId,productId){
       
        if(productId){
          // Make an AJAX call to notify the server to remove the product from the cart
          $.ajax({
            type: 'POST',  // or 'DELETE' depending on your API design
            url: '/removeProductFromCart/' + productId, // Replace with your actual API endpoint
            success: function(data) {
              if(data.removed==1){
                location.reload()
              }
            },
            error: function() {
              console.log("ajax error");
            }
          });
        }
