      <div class="container-fluid mt-3">
        <div class="row px-xl-5">
          <div class="col-lg-8 table-responsive mb-5">
            <table
              class="table table-light table-borderless table-hover text-center mb-0"
            >
              <thead style="background-color: rgb(0, 0, 0)">
                <tr>
                  <th style="color: white"></th>
                  <th style="color: white">Products</th>
                  <th style="color: white">Price</th>
                  <th style="color: white">Quantity</th>
                  <th style="color: white">Total</th>
                  <th style="color: white">Remove</th>
                </tr>
              </thead>

              <tbody>
                <% if (products == 0) { %>
                  <!-- Display a message if the cart is empty -->
                  <tr>
                    <td colspan="6">Your cart is empty.</td>
                  </tr>
                <% } else { %>
                
                <% for(let i=0;i<products.length;i++) { %>
                 
                  <tr style="background-color: rgb(235, 239, 241);">
                    <td style="padding-top: 0px;padding-bottom: 0px;">
                      <img src="/userImages/<%=products[i].product.image1%>" alt="image" style="width: 90px;height: 90px;">
                    
                      <td style="font-size: larger;font-weight: bold;"><%= products[i].product.name %></td>
                    </td>
                    <td style="font-size: larger;font-weight:bold;" id="singleProductPrice<%=products[i].product._id%>" >₹<%=products[i].product.price %></td>
                    <td>
                      <div class=" quantity mx-auto" style="padding-left: 25%;">
                        <button onclick="changeQty('<%=user_id%>','<%=products[i].product._id%>',-1)"  style="height: 30px; width: 70px; border: none;background-color:transparent;"><h4>-</h4></button>
                        <input type="text" id="qty<%=products[i].product._id%>" style="width: 30px;font-size: larger;font-weight: bold; text-align: center;" value="<%=products[i].quantity%>">
                        <button onclick="changeQty('<%=user_id%>','<%=products[i].product._id%>',1)" style="height: 30px; width: 70px; border: none;background-color:transparent;"><h4>+</h4></button>
                      </div>
                    </td>
                    

                    <td style="font-size: larger;font-weight: bold;" id="singleProductTotal<%=products[i].product._id%>"><p></p> ₹ <%=products[i].product.price %></td>
                    <td>
                      <button onclick="removeCartItem('<%=user_id%>','<%=products[i].product._id%>')" style="height: 40px; width: 40px; color: rgb(255, 255, 255);background-color: rgb(67, 67, 67);">X</button>
                    </td>
                    
                  </tr>
                <% } %>
                <% } %>
              </tbody>
            </table>
