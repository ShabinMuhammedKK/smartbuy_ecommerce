    <script>
       const options = document.querySelectorAll('input[type="radio"][name="options"]');
        const paymentOptions = document.querySelectorAll('input[type="radio"][name="payment"]');
        const submitBothSets = document.getElementById("submitBothSets");

        // Add event listener for the combined submit button
        submitBothSets.addEventListener("click", function () {
            // Initialize arrays to store selected data from both sets
            let selectedData;
            let selectedPaymentOptions;

            // Collect data from the first set of radio buttons
            options.forEach((option) => {
                if (option.checked) {
                    console.log("option checked");
                    selectedData=option.value;
                }
            });

            // Collect data from the second set of radio buttons
            paymentOptions.forEach((option) => {
                if (option.checked) {
                    // When a radio button is checked, add the selected payment option to the array
                    selectedPaymentOptions=option.id;
                }
            });

            // Combine both sets of data
            const combinedData = {
                selectedData: selectedData,
                selectedPaymentOptions: selectedPaymentOptions
            };

            // Create an XMLHttpRequest object for the combined data
            const xhr = new XMLHttpRequest();

            // Configure the request for the combined data
            xhr.open("POST", "/orderPlaceClicked", true);
            xhr.setRequestHeader("Content-Type", "application/json");

            // Define the callback function to handle the server's response for the combined data
            xhr.onload = function () {
                if (xhr.status === 200) {
                    // Request was successful, update the page as needed
                    const response = JSON.parse(xhr.responseText);
                    if (response.success === 'OrderPlaced') {
          
          $('#orderPlacedModal').modal('show');
        }else if(response.success === 'OnlinePayment'){
            console.log("online payment initialized");
        }
                }
            };

            // Convert the combined data to JSON and send it to the server
            xhr.send(JSON.stringify(combinedData));
            
        });
    </script>
      
      
